    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">fruit salad</ph></title>
    <shortdesc class="- topic/shortdesc "/><prolog class="- topic/prolog "><author class="- topic/author ">Anna Bureta</author></prolog>
    <taskbody class="- topic/body task/taskbody ">
        <context class="- topic/section task/context ">How to make a fruit salad.<p class="- topic/p "/></context>
        <steps class="- topic/ol task/steps ">
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Peel the oranges.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Wash the apples.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Rinse the strawberries with water.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Cut all the ingredients into neat 2-3cm cubes.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Mix the fruits in a bowl.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Add some lemon juice.</cmd>
            </step>
        </steps>
    </taskbody>
